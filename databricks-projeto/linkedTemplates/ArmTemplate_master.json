{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "databricks-projeto"
		},
		"DataLake_connectionString": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'connectionString' de 'DataLake'"
		},
		"sqlServerLink_password": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'password' de 'sqlServerLink'"
		},
		"LinkHttpsVm_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().BaseURL}"
		},
		"sqlServerLink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlprojeto01.database.windows.net"
		},
		"sqlServerLink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQLBI"
		},
		"sqlServerLink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pamelacaj"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databricks-projeto_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLake_connectionString": {
						"value": "[parameters('DataLake_connectionString')]"
					},
					"sqlServerLink_password": {
						"value": "[parameters('sqlServerLink_password')]"
					},
					"LinkHttpsVm_properties_typeProperties_url": {
						"value": "[parameters('LinkHttpsVm_properties_typeProperties_url')]"
					},
					"sqlServerLink_properties_typeProperties_server": {
						"value": "[parameters('sqlServerLink_properties_typeProperties_server')]"
					},
					"sqlServerLink_properties_typeProperties_database": {
						"value": "[parameters('sqlServerLink_properties_typeProperties_database')]"
					},
					"sqlServerLink_properties_typeProperties_userName": {
						"value": "[parameters('sqlServerLink_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}