{
	"name": "BinaryArqGitHub",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"NomeArquivo": {
				"type": "string",
				"defaultValue": "novo"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().NomeArquivo",
					"type": "Expression"
				},
				"folderPath": "inbound",
				"container": "arquivomainframe"
			},
			"compression": {
				"type": "ZipDeflate",
				"level": "Optimal"
			}
		}
	}
}