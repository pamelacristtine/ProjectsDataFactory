{
	"name": "DataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "Sink"
				}
			],
			"transformations": [
				{
					"name": "DerivedColumn"
				},
				{
					"name": "Select"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(CPF = substring(Column_1, 1, 11),",
				"          NOME = substring(Column_1, 12, 30),",
				"          DT_NASC = substring(Column_1, 42, 8),",
				"          UF = substring(Column_1, 50, 2),",
				"          SALDO_CTA = substring(Column_1, 52, 10)) ~> DerivedColumn",
				"DerivedColumn select(mapColumn(",
				"          CPF,",
				"          NOME,",
				"          DT_NASC,",
				"          UF,",
				"          SALDO_CTA",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Select",
				"Select sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Sink"
			]
		}
	}
}